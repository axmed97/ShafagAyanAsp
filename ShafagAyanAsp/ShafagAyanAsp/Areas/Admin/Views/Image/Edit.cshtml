@model Images
@{
    ViewData["Title"] = "Edit";
    ViewData["activeTab"] = "Image";
}

@section Title{
    <h4 class="page-title text-uppercase font-medium font-20">Image</h4>
}

<dl class="text-center w-100">
    <dt class="page-title text-capitalize font-medium font-20 mb-3">Current Image</dt>
    <dd><img src="~/image/@Model.Image" width="350" class="img-thumbnail" alt="Ayan Shafag" /></dd>
</dl>
<form method="post" enctype="multipart/form-data" class="w-100">
    <div class="form-group">
        <label asp-for="Photo"></label>
        <input asp-for="Photo" accept="image/" class="form-check" />
        <span asp-validation-for="Photo" class="text-danger"></span>
    </div>
    <input type="submit" value="Update" class="btn btn-outline-dark btn-block btn-lg" />
</form>

